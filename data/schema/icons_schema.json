{
  "$schema": "https://raw.githubusercontent.com/Mosberg/canvas/main/data/schema/icons_schema.json#",
  "type": "object",
  "properties": {
    "Icons": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer" },
          "src": { "type": "string" },
          "title": { "type": "string" },
          "alt": { "type": "string" },
          "type": {
            "type": "string",
            "enum": ["Combat", "Gathering", "Production", "Utility"]
          }
        },
        "required": ["id", "src", "title", "alt", "type"]
      }
    },
    "required": ["Icons"]
  }
}
