{
  "$schema": "https://raw.githubusercontent.com/Mosberg/canvas/main/data/schema/tools_schema.json#",
  "type": "object",
  "properties": {
    "Tools": {
      "type": "object",
      "properties": {
        "Axes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": { "type": "integer" },
              "level": { "type": "integer" },
              "src": { "type": "string" },
              "title": { "type": "string" },
              "alt": { "type": "string" }
            },
            "required": ["id", "level", "src", "title", "alt"]
          }
        },
        "Pickaxes": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "id": { "type": "integer" },
              "level": { "type": "integer" },
              "src": { "type": "string" },
              "title": { "type": "string" },
              "alt": { "type": "string" }
            },
            "required": ["id", "level", "src", "title", "alt"]
          }
        }
      },
      "required": ["Axes", "Pickaxes"]
    },
    "required": ["Tools"]
  }
}
