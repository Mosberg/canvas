{
  "$schema": "https://raw.githubusercontent.com/Mosberg/canvas/main/data/schema/gui_schema.json#",
  "type": "object",
  "properties": {
    "GUI": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "integer" },
          "src": { "type": "string" },
          "title": { "type": "string" },
          "alt": { "type": "string" }
